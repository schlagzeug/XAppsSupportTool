<Window x:Class="XAppsSupport.ListFLF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="List FLFs" Height="139" Width="244">
    <Grid Height="85" Width="210">
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="textBlock1" Text="File:" VerticalAlignment="Top" />
        <Button Content="Run" Height="23" HorizontalAlignment="Left" Margin="12,41,0,0" Name="button_run" VerticalAlignment="Top" Width="75" Click="button_run_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="72,9,0,0" Name="textBox_FileLocation" VerticalAlignment="Top" Width="120" />
        <RadioButton Content="C#" Height="16" HorizontalAlignment="Left" Margin="110,41,0,0" Name="radioButton_CSharp" VerticalAlignment="Top" IsChecked="True"/>
        <RadioButton Content="VBScript" Height="16" HorizontalAlignment="Left" Margin="110,57,0,0" Name="radioButton_VBScript" VerticalAlignment="Top" />
    </Grid>
</Window>
