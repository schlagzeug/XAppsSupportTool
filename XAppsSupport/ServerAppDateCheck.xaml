<Window x:Class="XAppsSupport.ServerAppDateCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ServerAppDateCheck" Height="600" Width="500">
    <Grid>
        <DataGrid AutoGenerateColumns="True"  HorizontalAlignment="Stretch" Margin="0,76,0,0" Name="dataGrid_Results" VerticalAlignment="Stretch" AlternatingRowBackground="LightBlue" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="56,43,0,0" Name="comboBox_Types" VerticalAlignment="Top" Width="329" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="111,14,0,0" Name="textBox_ProgramName" VerticalAlignment="Top" Width="355" />
        <Button Content="Check" Height="23" HorizontalAlignment="Left" Margin="391,43,0,0" Name="button_Check" VerticalAlignment="Top" Width="75" Click="button_Check_Click" />
        <Label Content="Program Name:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" />
        <Label Content="Type:" Height="28" HorizontalAlignment="Left" Margin="12,42,0,0" Name="label2" VerticalAlignment="Top" />
    </Grid>
</Window>
